<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>jQuery UI Autocomplete functionality</title>
    <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">
    <script src="https://code.jquery.com/jquery-1.10.2.js"></script>
    <script src="https://code.jquery.com/ui/1.10.4/jquery-ui.js"></script>

    <!-- Javascript -->
    <script>
        $(document).ready(function () {
            @{List<string> listFromController = (List<string>)ViewData["names"];}
                var Names = [
                        @for (int i = 0; i < listFromController.Count; i++)
                        {
                            @: '@(listFromController[i])',
                        }
                    ]
            $("#automplete-1").autocomplete({
                minLength: 2,
                source: Names
            });
        });
    </script>
</head>

<body>
    <div style="padding-top:10px; padding-bottom:10px">
        <i class="fas fa-user-circle" style="font-size: 28px; color:#00295C; display:inline"></i><h2 style="color:#00295C; display: inline;">@Html.ActionLink("  Admin Omgeving", "Index")</h2> <h3 style="color:#00295C; display: inline;"> > Nieuwe Lijn Grafiek</h3>
    </div>

    @using (Html.BeginForm("CreateGrafiekLine", "Admin"))
    {
        <div class="ui-widget">
            <label for="automplete-1">Naam: </label>
            <input type="text" name="automplete" id="automplete-1">
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </div>
    }
</body>
</html>